{
  "coc.preferences.formatOnSaveFiletypes": [
    "json",
    "go",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "terraform"
  ],
  "suggest.keepCompleteopt": true,
  "eslint.autoFixOnSave": true,
  "languageserver": {
